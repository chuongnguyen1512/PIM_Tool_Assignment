<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="boxLeft" prefHeight="700" prefWidth="300" style="-fx-border-color: grey; -fx-border-width: 0 0.5 0 0;" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="btnOpenProject" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#openProjectList" prefHeight="47.0" text="%btnProjectslist" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets right="20.0" top="30.0" />
            </VBox.margin>
            <font>
                <Font size="18.0" />
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </Button>
        <Button fx:id="btnNew" alignment="TOP_LEFT" mnemonicParsing="false" text="%btnNew">
            <font>
                <Font size="18.0" />
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <VBox.margin>
                <Insets right="20.0" top="10.0" />
            </VBox.margin>
        </Button>
        <VBox fx:id="vboxChildNew" prefHeight="200.0" visible="false">
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets top="10.0" />
            </padding>
            <children>
                <Button fx:id="btnNewProject" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#openNewProject" prefWidth="200.0" styleClass="vboxChilds" text="%btnNewProject">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <padding>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
                    </padding>
                </Button>
                <Button alignment="TOP_LEFT" mnemonicParsing="false" prefWidth="200.0" styleClass="vboxChilds" text="%btnNewCustomer">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <padding>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
                    </padding>
                </Button>
                <Button alignment="TOP_LEFT" mnemonicParsing="false" prefWidth="200.0" styleClass="vboxChilds" text="%btnNewSupplier">
                    <font>
                        <Font size="14.0" />
                    </font>
                    <padding>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
                    </padding>
                </Button>
            </children>
        </VBox>
    </children>
   <padding>
      <Insets left="80.0" />
   </padding>
</VBox>
