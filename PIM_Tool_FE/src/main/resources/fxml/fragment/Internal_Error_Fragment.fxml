<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="70" AnchorPane.topAnchor="10"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox prefHeight="500.0" prefWidth="1000.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="20.0"
              AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0">
            <children>
                <ImageView fx:id="imgViewErrorIcon" fitHeight="150.0" fitWidth="300.0" pickOnBounds="true"
                           preserveRatio="true" style="-fx-background-color: transparent;"/>
                <VBox prefHeight="200.0" prefWidth="600.0">
                    <children>
                        <Label alignment="TOP_LEFT" prefHeight="0.0" prefWidth="500.0" text="%lbUnexpected"
                               wrapText="true">
                            <VBox.margin>
                                <Insets bottom="5.0" top="5.0"/>
                            </VBox.margin>
                            <font>
                                <Font name="System Bold" size="18.0"/>
                            </font>
                        </Label>
                        <HBox prefWidth="200.0">
                            <children>
                                <Label text="%lbPlease">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Label>
                                <Label text="%lbContact" textFill="#ee6c6c">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Label>
                            </children>
                            <VBox.margin>
                                <Insets bottom="5.0" top="5.0"/>
                            </VBox.margin>
                        </HBox>
                        <HBox prefWidth="200.0">
                            <children>
                                <Label text="%lbOr">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="lbBackToProjectList" text="%lbBackToSearch" textFill="#49a4ff">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Label>
                            </children>
                            <VBox.margin>
                                <Insets top="10.0"/>
                            </VBox.margin>
                        </HBox>
                    </children>
                    <HBox.margin>
                        <Insets left="50.0"/>
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
